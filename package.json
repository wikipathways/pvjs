{
  "name": "pvjs",
  "version": "3.0.0-beta",
  "description": "Diagram viewer and editor for biological pathways.",
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run compile:ts",
    "gulp": "gulp",
    "compile:ts": "tsc -p tsconfig.json && npm run copy:assets",
    "copy:assets": "npm run gulp copy:assets",
    "build": "webpack --config webpack.config.js",
    "start": "npm-run-all -p start:mockserver start:webpack",
    "start:mockserver": "./node_modules/mockserver/bin/mockserver.js -p 4522 -m './test/mockserver-data'",
    "start:webpack": "webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "start:svgfilter": "webpack-dev-server --config webpack.svgfilter.js --progress --colors --watch",
    "test": "echo 'add working tests'",
    "watch": "npm-run-all -p start:mockserver watch:*",
    "watch:client": "npm-watch",
    "watch:webpack": "npm run start:webpack",
    "watch:input-data-broken": "cat <(cat test/mockserver-data/pathways/HEADERS.txt) <(echo -n '{\"data\":\"') <(cat test/input-data/dev/playground.gpml | base64 | xargs echo -n) <(echo -n '\"}') > test/mockserver-data/pathways/playground/GET.mock"
  },
  "watch": {
    "test": [
      "main.js",
      "test/*.{js,jsx}"
    ]
  },
  "browser": {
    "commander": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikipathways/pvjs.git"
  },
  "keywords": [
    "pvjs",
    "network",
    "pathway",
    "biology"
  ],
  "author": "WikiPathways team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wikipathways/pvjs/issues"
  },
  "homepage": "https://wikipathways.github.io/pvjs/docs/",
  "dependencies": {
    "@webcomponents/custom-elements": "^1.0.0-alpha.3",
    "bootstrap": "^3.3.7",
    "bridgedb": "^6.0.0-14",
    "csstips": "^0.2.0",
    "csv-streamify": "^1.0.0",
    "csx": "^8.2.1",
    "custom-elements": "^1.0.2",
    "d3": "^4.7.4",
    "gpml2pvjson": "2.0.0-beta.10",
    "js-base64": "^2.1.9",
    "lodash": "^4.14.36",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dom": "^15.3.2",
    "react-selectize": "^2.1.0",
    "react-spinkit": "^2.1.1",
    "rgbcolor": "^1.0.0",
    "rx-extra": "^2.0.3",
    "rxjs": "^5.2.0",
    "svg-pan-zoom": "^3.4.1",
    "svg-text": "^0.4.6",
    "tower-strcase": "~0.5.0",
    "typestyle": "^0.22.3",
    "url-loader": "^0.5.8",
    "valid-data-url": "^0.1.3",
    "web-animations-js": "^2.2.2"
  },
  "devDependencies": {
    "@types/assert": "0.0.28",
    "@types/bootstrap": "^3.3.32",
    "@types/js-base64": "^2.1.5",
    "@types/jsonstream": "^0.8.27",
    "@types/node": "^6.0.68",
    "@types/react": "^15.0.20",
    "@types/react-dom": "^0.14.23",
    "@types/urijs": "^1.15.27",
    "babel-preset-es2015": "^6.13.2",
    "bdd-with-opts": "~1.0.0",
    "blessed": "^0.1.81",
    "browser-sync": "^2.13.0",
    "chai": "~1.10.0",
    "chai-as-promised": "~4.1.1",
    "colors": "~1.0.3",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.26.1",
    "deep-diff": "^0.3.3",
    "deglobalify": "^0.2.0",
    "file-loader": "^0.10.1",
    "freeport": "~1.0.4",
    "global": "^4.3.0",
    "graceful-fs": "^4.1.4",
    "gulp": "^3.9.1",
    "gulp-copy": "^1.0.0",
    "html-webpack-plugin": "^2.24.0",
    "image-webpack-loader": "^3.2.0",
    "inquirer": "~0.8.0",
    "jaguarjs-jsdoc": "0.0.1",
    "jsdoc": "^3.4.0",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-firefox-launcher": "0.1.7",
    "karma-growl-reporter": "0.1.1",
    "karma-mocha": "0.2.2",
    "karma-mocha-reporter": "2.0.0",
    "mkdirp": "~0.5.1",
    "mocha": "^2.5.3",
    "mockserver": "^1.5.1",
    "npm-run-all": "^2.1.1",
    "npm-watch": "^0.1.6",
    "pretty-data": "~0.40.0",
    "pretty-hrtime": "~0.2.2",
    "react-addons-test-utils": "^15.3.2",
    "rsyncwrapper": "~0.4.1",
    "rx-fs": "ariutta/rx-fs#03425b37820b71ab4e8947a6e14fa5ef8135e990",
    "sax": "~0.6.1",
    "selenium-launcher": "~1.1.10",
    "sinon": "~1.12.1",
    "spawn-mocha-parallel": "~1.0.0",
    "style-loader": "^0.13.1",
    "svg-url-loader": "^2.0.2",
    "through": "~2.3.6",
    "ts-loader": "^1.3.3",
    "typescript": "^2.2.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0",
    "yargs": "~1.3.3"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib",
        "./demo",
        "./gulp",
        "./dist"
      ]
    }
  }
}
